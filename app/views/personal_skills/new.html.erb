<div ng-app="app">
	<div ng-controller="resumeCtrl">

		<div class="section">
			<div class="container">
				<div class="row">
					<div class="col-md-12">
						<div class="panel panel-primary">
							<div class="panel-heading">
								<h3 class="panel-title">Personal Skills</h3>
							</div>
							<div class="panel-body">
								<form role="form" method="get" action="<%= new_reference_path %>">
									<h4>Example: Very Organized</h4>
									<div ng-repeat="skill in personalSkills">
										<div class="row">
											<div class="form-group col-md-4">
												<input class="form-control" type="text" ng-click="anotherPersonalSkillForm(skill)" ng-model="skill.skillKey">
											</div>
											<div ng-if="skill.skillKey" class="form-group col-md-4">
												<label class="remove" ng-click="removePersonalSkillForm(skill)"><i class="fa fa-2x fa-times-circle-o"></i></label>
											</div>
										</div>
									</div>
									<button type=" submit "="=" " "="" ng-click="addNewPersonalSkills(personalSkills)">Submit</button>
								</form>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		
	</div>
</div>