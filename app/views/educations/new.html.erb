										  <div class="form-group col-md-11" id="double_column">
									        <i class="fa fa-1x fa-plus fa-fw "></i>
									        <label class="control-label">Add Another Education</label>
									      <!-- <br> -->
										  </div>
										  <div class="form-group col-md-1" id="double_column">
										  <!-- <a href=<%= new_education_path %>> -->
									        <button type=" submit "="=" " "="" ng-click="addNewEducation(degreeEarned, universityName, startDate, endDate)">Submit</button>
										  <!-- </a> -->
									      </div>
									    </div>
									</form>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>

		</div>
	</div>